{
  "name": "morpho-blue",
  "displayName": "Morpho Blue",
  "description": "A minimal VSCode theme with teal accents.",
  "icon": "assets/icon.png",
  "version": "0.0.8",
  "publisher": "BigFootForward",
  "repository": {
    "type": "git",
    "url": "https://github.com/bfforward/morpho-blue-vscode-theme"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark",
    "teal",
    "morpho",
    "blue"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Morpho Blue Dark",
